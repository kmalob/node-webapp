pipeline {
    agent any
    stages {
        stage('Git') {
            agent any
            steps {
                git branch: 'main', credentialsId: '6a64497d-d665-4954-9ce4-e8221175197f', url: 'http://github.com/kmalob/node-webapp'
                sh 'docker build -t node_app .'
            }
        }
        stage('Run') {
            agent any {
                sh 'cd node-express-hello-world'
                sh 'npm install'
                sh 'npm start'
            }
        }

    }
}
